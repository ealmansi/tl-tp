program : seq_fun_def plot_stmt
        ;

seq_fun_def : fun_def
            | seq_fun_def fun_def
            ;

plot_stmt : KW_PLOT LPAREN fun_call COMMA fun_call RPAREN KW_FOR ID EQUAL LITERAL ELLIPSIS LITERAL ELLIPSIS LITERAL
          ;

fun_def : KW_FUNCTION ID LPAREN lst_id RPAREN block
        | KW_FUNCTION ID LPAREN RPAREN block
        ;

fun_call  : ID LPAREN lst_expr RPAREN
          | ID LPAREN RPAREN
          ;

lst_id  : ID
        | lst_id COMMA ID
        ;

lst_expr  : expr
          | lst_expr COMMA expr
          ;

block : stmt
      | LBRACE seq_stmt RBRACE
      ;

stmt  : KW_RETURN expr
      ;

seq_stmt  : stmt
          | seq_stmt stmt
          ;

expr  : LITERAL
      ;