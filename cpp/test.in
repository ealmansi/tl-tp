function fact(n) {
  if n == 0 || n == 1 then
    return 1
  i = 1
  while n > 0 {
    i = i * n
    n = n - 1
  }
  return i
}

function sin(x) {
  x2 = x*x
  x3 = x*x2
  x5 = x3*x2
  x7 = x5*x2
  x9 = x7*x2
  return x - x3/fact(3) + x5/fact(5) - x7/fact(7) + x9/fact(9) /* primeros 5 terminos no nulos del polinomio de Taylor */
}

function id(x)
  return x

plot (id(x), sin(x)) for x=-pi..0.1..pi